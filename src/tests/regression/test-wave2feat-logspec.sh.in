#!/bin/sh

tmpout="test-wave2feat-logspec.out"

echo "WAVE2FEAT-LOGSPEC TEST"
sh @abs_top_builddir@/libtool --mode=execute @abs_top_builddir@/src/programs/wave2feat \
-logspec 1 \
-samprate 11025 \
-frate 105 \
-wlen 0.024 \
-alpha 0.97 \
-nfft 512 \
-nfilt 36 \
-upperf 5400 \
-lowerf 130 \
-blocksize 262500 \
-i @abs_top_srcdir@/model/ep/chan3.raw \
-o test-wave2feat-logspec.mfc.out  \
-raw 1 > $tmpout 2>&1 

sh @abs_top_builddir@/libtool --mode=execute @abs_top_builddir@/src/programs/cepview \
-i 40 \
-d 40 \
-f test-wave2feat-logspec.mfc.out \
> test-wave2feat-logspec.cepview.out

if (perl @abs_top_srcdir@/src/tests/regression/compare_table.pl test-wave2feat-logspec.cepview.out @abs_top_srcdir@/model/ep/chan3-logspec.cepview | grep SUCCESS > /dev/null 2>&1) ; \
then echo "WAVE2FEAT-LOGSPEC test PASSED"; else \
echo "WAVE2FEAT-LOGSPEC test FAILED"; fi

