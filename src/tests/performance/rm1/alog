rm -f gmake-rm1_flat_unigram.results
/bin/cp ARGS.rm1 ARGS.rm1_flat_unigram
/bin/echo "-lm RM.unigram.arpa.DMP" >> ARGS.rm1_flat_unigram
/bin/echo "-lw 7" >> ARGS.rm1_flat_unigram
/bin/echo "-beam 1e-140" >> ARGS.rm1_flat_unigram
/bin/echo "-wbeam 1e-80" >> ARGS.rm1_flat_unigram
/bin/echo "-ctl " >> ARGS.rm1_flat_unigram
../batchmetrics ./rm1.batch / ./ARGS.rm1_flat_unigram > gmake-rm1_flat_unigram.results
